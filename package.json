{
  "name": "emr-tools-v2",
  "displayName": "emr-tools",
  "description": "The extension for developers building applications to run in EMR clusters.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:emr-tools-v2.helloWorld",
	"onView:emrExplorer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "emr-tools-v2.helloWorld",
        "title": "Hello World"
      }
    ],
    "views": {
      "emr-explorer": [
        {
          "id": "emrExplorer",
          "name": "EMR Explorer"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "emr-explorer",
          "title": "Amazon EMR",
          "icon": "media/emr.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@aws-sdk/client-emr": "^3.58.0"
  }
}
